<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cipherresource.project.system.miccert.mapper.TMiccertMapper">
    
    <resultMap type="TMiccert" id="TMiccertResult">
        <result property="id"    column="id"    />
        <result property="type"    column="type"    />
        <result property="status"    column="status"    />
        <result property="version"    column="version"    />
        <result property="createdate"    column="createdate"    />
        <result property="certsn"    column="certsn"    />
        <result property="notvaliddate"    column="notValidDate"    />
        <result property="pubkey"    column="pubkey"    />
        <result property="cert"    column="cert"    />
    </resultMap>

    <sql id="selectTMiccertVo">
        select id, type, status, version, createdate, certsn, notValidDate, pubkey, cert from t_miccert
    </sql>

    <select id="selectTMiccertList" parameterType="TMiccert" resultMap="TMiccertResult">
        <include refid="selectTMiccertVo"/>
        <where>  
            <if test="type != null "> and type = #{type}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="version != null  and version != ''"> and version = #{version}</if>
            <if test="createdate != null "> and createdate = #{createdate}</if>
            <if test="certsn != null  and certsn != ''"> and certsn = #{certsn}</if>
            <if test="notvaliddate != null "> and notValidDate = #{notvaliddate}</if>
            <if test="pubkey != null "> and pubkey = #{pubkey}</if>
            <if test="cert != null  and cert != ''"> and cert = #{cert}</if>
        </where>
    </select>
    
    <select id="selectTMiccertById" parameterType="Long" resultMap="TMiccertResult">
        <include refid="selectTMiccertVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTMiccert" parameterType="TMiccert" useGeneratedKeys="true" keyProperty="id">
        insert into t_miccert
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="type != null ">type,</if>
            <if test="status != null ">status,</if>
            <if test="version != null  and version != ''">version,</if>
            <if test="createdate != null ">createdate,</if>
            <if test="certsn != null  and certsn != ''">certsn,</if>
            <if test="notvaliddate != null ">notValidDate,</if>
            <if test="pubkey != null ">pubkey,</if>
            <if test="cert != null  and cert != ''">cert,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="type != null ">#{type},</if>
            <if test="status != null ">#{status},</if>
            <if test="version != null  and version != ''">#{version},</if>
            <if test="createdate != null ">#{createdate},</if>
            <if test="certsn != null  and certsn != ''">#{certsn},</if>
            <if test="notvaliddate != null ">#{notvaliddate},</if>
            <if test="pubkey != null ">#{pubkey},</if>
            <if test="cert != null  and cert != ''">#{cert},</if>
         </trim>
    </insert>
    
        <insert id="insertTMiccertHistory" parameterType="TMiccert" useGeneratedKeys="true" keyProperty="id">
        insert into t_miccert_his
        <trim prefix="(" suffix=")" suffixOverrides=",">
        	<if test="id != null ">id,</if>
            <if test="type != null ">type,</if>
            <if test="status != null ">status,</if>
            <if test="version != null  and version != ''">version,</if>
            <if test="createdate != null ">createdate,</if>
            <if test="certsn != null  and certsn != ''">certsn,</if>
            <if test="notvaliddate != null ">notValidDate,</if>
            <if test="pubkey != null ">pubkey,</if>
            <if test="cert != null  and cert != ''">cert,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
        	<if test="id != null ">#{id},</if>
            <if test="type != null ">#{type},</if>
            <if test="status != null ">#{status},</if>
            <if test="version != null  and version != ''">#{version},</if>
            <if test="createdate != null ">#{createdate},</if>
            <if test="certsn != null  and certsn != ''">#{certsn},</if>
            <if test="notvaliddate != null ">#{notvaliddate},</if>
            <if test="pubkey != null ">#{pubkey},</if>
            <if test="cert != null  and cert != ''">#{cert},</if>
         </trim>
    </insert>

    <update id="updateTMiccert" parameterType="TMiccert">
        update t_miccert
        <trim prefix="SET" suffixOverrides=",">
            <if test="type != null ">type = #{type},</if>
            <if test="status != null ">status = #{status},</if>
            <if test="version != null  and version != ''">version = #{version},</if>
            <if test="createdate != null ">createdate = #{createdate},</if>
            <if test="certsn != null  and certsn != ''">certsn = #{certsn},</if>
            <if test="notvaliddate != null ">notValidDate = #{notvaliddate},</if>
            <if test="pubkey != null ">pubkey = #{pubkey},</if>
            <if test="cert != null  and cert != ''">cert = #{cert},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTMiccertById" parameterType="Long">
        delete from t_miccert where id = #{id}
    </delete>

    <delete id="deleteTMiccertByIds" parameterType="String">
        delete from t_miccert where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>